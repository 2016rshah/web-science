{
    "manifest_version": 2,
    "name": "Data",
    "version": "1.0",

    "description": "Collect specified browsing information",
    "icons": {
        "48": "icons/eye-48.png"
    },

    "options_ui": {
        "page": "settings/options.html"
    },
    "content_scripts": [
    {

        "matches": [
            "<all_urls>"
        ],
        "css":["scripts/jquery-ui.css"],
        "js": [
            "scripts/jquery.js",
            "scripts/jquery-ui.js",
            "scripts/newsLinks.js",
            "scripts/process.js"
        ]
    }
    ],
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage"
    ],


    "browser_specific_settings": {
        "gecko": {
            "id": "collect@example.com"
        }
    }

}
