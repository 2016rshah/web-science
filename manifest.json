{

    "manifest_version": 2,
    "name": "Data",
    "version": "1.0",

    "description": "Collect specified browsing information",
    "icons": {
        "48": "icons/eye-48.png"
    },

    "permissions": [
        "<all_urls>",
        "tabs",
        "storage"
    ],

    "background": {
        "scripts": ["scripts/collection.js"]
    },

    "browser_specific_settings": {
        "gecko": {
            "id": "collect@example.com"
        }
    }

}
